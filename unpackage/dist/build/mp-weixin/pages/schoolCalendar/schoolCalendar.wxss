.tabbar{height:100rpx;line-height:100rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-justify-content:space-around;justify-content:space-around}.isSelect{color:#c99;border-bottom:4rpx solid #c99}.noSelect{color:grey}.content{background-color:#f1f1f1;height:calc(100vh - 100rpx)}.content image{width:90%;margin:20% 5%}.tips{color:grey;font-size:26rpx;position:fixed;left:20%;bottom:10%}