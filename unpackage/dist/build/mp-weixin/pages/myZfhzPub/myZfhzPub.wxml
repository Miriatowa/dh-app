<view class="index"><view class="collectionItem"><view class="itemTop"><view>共计发布<text style="color:#D43C43;padding:0rpx 5rpx;">{{courseCollectionArr.length}}</text>条</view><view data-event-opts="{{[['tap',[['onEdit',['$event']]]]]}}" style="color:#D43C43;padding:20rpx;" bindtap="__e">{{''+editText+''}}</view></view><view class="itemMid"><checkbox-group data-event-opts="{{[['change',[['singleSelect',['$event']]]]]}}" bindchange="__e"><block wx:for="{{courseCollectionArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="billItem"><checkbox value="{{item.houseId}}" checked="{{item.check}}" hidden="{{!isEdit}}"></checkbox><view class="zfItem"><view class="zfItemLeft"><view class="zfTitle">{{item.houseTitle}}</view><view class="zfAddress">{{"期望区域："+item.houseAddress}}</view><view class="zfContact">{{item.contactType+"："+item.contactNumber}}</view><view class="zfTime">{{"发布时间："+item.publishTime}}</view></view><view class="zfItemRight"><view class="zfPrice">{{"￥ "+item.housePrice+"/月"}}</view></view></view></view></block></checkbox-group></view></view><view class="itemBottom"><view class="bottomBar" hidden="{{!isEdit}}"><checkbox-group data-event-opts="{{[['change',[['allSelect',['$event']]]]]}}" bindchange="__e"><checkbox value="all" checked="{{checkkAll}}">全选</checkbox></checkbox-group><button class="cancelBtn" type="warn" data-event-opts="{{[['tap',[['onCancelCollection',['$event']]]]]}}" bindtap="__e">取消发布</button></view></view></view>