<view class="index"><view class="collectionItem"><view class="collectionItemTop"><view>共计发布<text style="color:#D43C43;padding:0rpx 5rpx;">{{courseCollectionArr.length}}</text>条</view><view data-event-opts="{{[['tap',[['onEdit',['$event']]]]]}}" style="color:#D43C43;padding:20rpx;" bindtap="__e">{{''+editText+''}}</view></view><view class="collectionItemMid"><checkbox-group data-event-opts="{{[['change',[['singleSelect',['$event']]]]]}}" bindchange="__e"><block wx:for="{{courseCollectionArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="billItem"><checkbox value="{{item.id}}" checked="{{item.check}}" hidden="{{!isEdit}}"></checkbox><view class="item"><view class="itemTop"><view class="serviceType">{{item.publishType}}</view><view class="serviceContent">{{"￥ "+item.price}}</view></view><view class="itemMid"><view><view class="serviceAddress"><label class="oriTitle _span">起</label>{{item.orignPlace}}</view><view class="serviceAddress"><label class="aimTitle _span">终</label>{{item.aimsPlace}}</view></view><view class="serivceDes"><icon class="iconfont icon-wupin"></icon>{{"："+item.publishDescribe+''}}</view></view><view class="itemBot"><view class="contact"><icon class="iconfont icon-shouji"></icon>{{item.contactType+": "+item.contactNum}}</view><view class="time">{{item.publishTime}}</view></view></view></view></block></checkbox-group></view></view><view class="itemBottom"><view class="bottomBar" hidden="{{!isEdit}}"><checkbox-group data-event-opts="{{[['change',[['allSelect',['$event']]]]]}}" bindchange="__e"><checkbox value="all" checked="{{checkkAll}}">全选</checkbox></checkbox-group><button class="cancelBtn" type="warn" data-event-opts="{{[['tap',[['onCancelCollection',['$event']]]]]}}" bindtap="__e">取消发布</button></view></view></view>