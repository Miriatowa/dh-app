	margin-right: 10rpx;
}
.aimTitle{
	background-color: #F0AD4E;
	padding: 5rpx 10rpx;
	font-size: 26rpx;
	color: #FFFFFF;
	border-radius: 50%;
	margin-right: 10rpx;
}
.itemBot{
	padding-bottom: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
