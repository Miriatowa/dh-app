	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.contact{
	margin-left: 10rpx;
	font-size: 26rpx;
	color: #808080;
}
.time{
	margin-right: 20rpx;
	font-size: 26rpx;
	color: #808080;
	margin-top: 16rpx;
}

