<view><view class="good_details"><block wx:for="{{goodArray}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block class="get_data"><swiper class="swiper" indicator-dots="{{true}}" circular="{{true}}" autoplay="true" indicator-active-color="#CC9999"><block wx:for="{{item.goodsUrl}}" wx:for-item="item2" wx:for-index="index" wx:key="index"><swiper-item><view class="good_img"><image src="{{item2}}" mode="aspectFit"></image></view></swiper-item></block></swiper><view class="good_content"><view class="good_name"><text>{{item.goodsName}}</text></view><view class="good_seller_name"><text>{{"卖家："+item.nickName}}</text></view><view class="good_seller_price"><text>{{"价格："+item.goodsPrice}}</text></view><view class="good_qq"><text selectable="true">{{item.contactType+"："+item.contactNum}}</text><button style="display:none;" bindtap="copy">复制</button></view><view class="good_data"><text>{{"发布时间: "+item.publishTime}}</text></view><view class="good_number"><text>{{"详情描述："+item.goodsDescribe}}</text></view></view></block></block><view class="btm_tool_wrap"><view class="btm_item share"><text class="iconfont icon-fenxiang" style="font-size:60rpx;"></text><button open-type="share">分享</button></view><navigator class="btm_item cart" hover-class="none" url="../myCollection/myCollection"><text class="iconfont icon-shoucang" style="font-size:54rpx;"></text></navigator><view data-event-opts="{{[['tap',[['handleItemAdd',['$event']]]]]}}" class="btm_item add_collection" bindtap="__e">加入收藏</view><view data-event-opts="{{[['tap',[['handleItemCon',['$event']]]]]}}" class="btm_item contact_now" bindtap="__e"><icon class="iconfont icon-lianximaijia" style="margin-right:8rpx;margin-bottom:5rpx;"></icon>联系卖家</view></view></view></view>