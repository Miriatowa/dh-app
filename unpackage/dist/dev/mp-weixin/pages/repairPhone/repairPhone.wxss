
.uni-list{
	background-color: #CC9999;
	width: 100%;
	height: 100rpx;
}
.uni-list-cell{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	background-color: #FFFFFF;
	height: 80rpx;
	width: 90%;
	-webkit-border-radius: 10rpx;
	        border-radius: 10rpx;
	margin-left: 5%;
	line-height: 80rpx;
}
.uni-list-cell-left{
	width: 140rpx;
	font-size: 28rpx;
	margin-left: 20rpx;
	color: #808080;
}
.uni-list-cell-db{
	-webkit-box-flex:1;
	-webkit-flex:1;
	        flex:1;
}
.uni-list-cell-db picker{
	padding-left: 150rpx;
	color: #CC9999;
}
.bottomContent{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.contentItem{
	height: 100rpx;
	line-height: 100rpx;
	font-size: 28rpx;
	text-align: center;
	color: #505050;
}
.selectContentItem{
	height: 100rpx;
	line-height:  100rpx;
	font-size: 30rpx;
	text-align: center;
	background-color: #FFFFFF;
	color: #CC9999;
}
.contentLeft{
	height: calc(100vh - 100rpx);
	width: 220rpx;
	background-color: #F1F1F1;
}
.contentRight{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.title{
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	padding-left: 20px;
	background: #f5f5f5;
	color: #646464;
	font-size: 28rpx;
	/* border-bottom: 2rpx solid #808080; */
}
.detail{
	height: 140rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	color: #808080;
}
.leftDetail{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.itemName{
	margin-top: 8rpx;
	font-size: 22rpx;
	line-height: 50rpx;
}
.roleImg{
	width: 80rpx;
	height: 80rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	margin-right: 10rpx;
}

